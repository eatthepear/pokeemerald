script Zone0C_EventScript_VerifyDestination {
	msgbox(format("Are you sure you want to go to {COLOR RED}Zone {STR_VAR_1}{COLOR DARK_GRAY}?"), MSGBOX_YESNO)
	if (var(VAR_RESULT) == NO) {
		msgbox(format("Decided not to warp."))
		releaseall
		end
	}
	setflag(FLAG_IS_REVISITING_ZONE)
	return
}

script Zone0C_EventScript_DenyWarp {
	lockall
	msgbox(format("The warp doesn't seem to workâ€¦"))
	releaseall
}

script Zone0C_EventScript_WarpZone1 {
	lockall
	if (var(VAR_ZONE) < 2) {
		goto(Zone0C_EventScript_DenyWarp)
	}
	buffernumberstring(0, 1)
	call(Zone0C_EventScript_VerifyDestination)
	setvar(VAR_REVISITING_ZONE, 1)
	warpteleport(MAP_ZONE1A, 255, 16, 28)
	waitstate
}

