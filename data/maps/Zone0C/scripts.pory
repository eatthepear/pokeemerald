script Zone0C_EventScript_WarpBack {
	lockall
	if (var(VAR_ZONE_0_STATE) == 3) {
		setvar(VAR_ZONE_0_STATE, 4)
	}
	switch (var(VAR_ZONE)) {
		case 1:
			warpteleport(MAP_ZONE0SANCTUARY, 255, 22, 12)
