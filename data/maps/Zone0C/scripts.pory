mapscripts Zone0C_MapScripts {
	MAP_SCRIPT_ON_FRAME_TABLE [
		VAR_ZONE_0_STATE, 2: Zone0C_EventScript_Intro
		// VAR_ZONE_B1_STATE, 1: Zone0C_EventScript_B1WarpIntro
	]
}
 
script Zone0C_EventScript_Intro {
	lockall
	applymovement(1, Common_Movement_ExclamationMark)
	playse(SE_PIN)
	waitmovement(0)
	waitse
	msgbox(format("A test subject! Perfect!"))
	applymovement(1, Common_Movement_WalkDown)
	waitmovement(0)
	applymovement(1, Common_Movement_WalkDown)
	waitmovement(0)
	applymovement(1, Common_Movement_WalkDown)
	waitmovement(0)
	msgbox(Zone0C_Text_Intro)
	applymovement(1, Zone0C_Movement_PostIntro)
	waitmovement(0)
	setvar(VAR_ZONE_0_STATE, 3)
	releaseall
}

text Zone0C_Text_Intro {
	format("Welcome to the Teleport Institute, or TPI for short.\p"
	"There are warps that will bring you to any area you've already visited.\p"
	"I'm sure you'll get the hang of it.\p"
	"If you need to get back to where you came from, you can use the warp immediately to our left.")
}

script Zone0C_EventScript_Scientist {
	lock
	faceplayer
	msgbox(Zone0C_Text_Explain)
	release
}

text Zone0C_Text_Explain {
	format("The warps are hardwired to only teleport people to places they have been before.\p"
	"If you need to get back to where you came from, you can use the warp immediately to our left.")
}

